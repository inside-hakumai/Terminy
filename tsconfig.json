{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitThis": true,
    "outDir": "./main/main",
    "types": [
      "node"
    ]
  },
  "include": [
    "./src/main/*.ts"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "./src/main/models/*"
  ]
}
